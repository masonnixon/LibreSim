        "interval": 0.5
      },
      "inputPorts": [
        { "id": "quant1-in", "name": "in", "dataType": "double", "dimensions": [1] }
      ],
      "outputPorts": [
        { "id": "quant1-out", "name": "out", "dataType": "double", "dimensions": [1] }
      ]
    },
    {
      "id": "rate_quant",
      "type": "rate_limiter",
      "name": "Rate + Quant",
      "position": { "x": 420, "y": 320 },
      "parameters": {
        "risingLimit": 10,
        "fallingLimit": -10
      },
      "inputPorts": [
        { "id": "rate_quant-in", "name": "in", "dataType": "double", "dimensions": [1] }
